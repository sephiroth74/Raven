package com.aviary.geom.intersection
{
	public class IntersectionResult
	{
		public var new_paths: Array;
		public var untouched_paths: Array;
		public var intersection_points: Array;
		public var approx_paths: Array;
		
		function IntersectionResult( npaths: Array, upaths: Array, ipoints: Array = null, apaths: Array = null )
		{
			new_paths = npaths;
			untouched_paths = upaths;
			intersection_points = ipoints != null ? ipoints : new Array( );
			approx_paths = apaths != null ? apaths : new Array( );
		}
		
	}
}